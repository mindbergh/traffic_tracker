{
  "manifest_version": 2,

  "name": "Traffic Tracker",
  "description": "Track time it takes to drive from one place to another.",
  "version": "1.0",

  "background": {
      "scripts": ["js/background.js"],
      "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "alarms"
  ]
}
